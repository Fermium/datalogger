{
  "name": "Datalogger",
  "version": "1.0.0",
  "description": "A datalogger application",
  "main": "app/main.js",
  "scripts": {
    "start": " electron .",
    "rebuild": "electron-rebuild",
    "pack": "build --dir",
    "dist": "build",
    "dist:mac": "build --mac",
    "dist:win": "build --win",
    "dist:linux": "build --linux"
  },
  "postinstall": "install-app-deps",
  "repository": "https://github.com/fermiumlabs/datalogger",
  "keywords": [],
  "author": {
    "name": "Simone Tosato",
    "email": "s@fermiumlabs.com",
    "url": "https://fermiumlabs.com"
  },
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-react": "^6.23.0",
    "electron": "^1.6.6",
    "electron-builder": "^16.6.0",
    "electron-builder-squirrel-windows": "^16.6.0",
    "electron-rebuild": "^1.5.7",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-jshint": "^1.12.0",
    "gulp-less": "^3.0.3",
    "gulp-minify-css": "^1.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.4.2",
    "rebuild": "^0.1.2"
  },
  "dependencies": {
    "bootstrap-slider": "^9.7.2",
    "chart.js": "^2.5.0",
    "codemirror": "^5.25.0",
    "d3": "^4.7.3",
    "data-chan": "^0.2.7",
    "dateformat": "^2.0.0",
    "easytimer": "^1.1.1",
    "echarts": "^3.4.0",
    "electron-pdf-window": "^1.0.10",
    "electron-titlebar": "0.0.3",
    "fs-path": "0.0.23",
    "js-yaml": "^3.8.2",
    "json2csv": "^3.7.3",
    "json2xls": "^0.1.2",
    "jsonfile": "^2.4.0",
    "lodash": "^4.17.4",
    "lowdb": "^0.16.0",
    "mathjax-electron": "^1.2.0",
    "mathjs": "^3.10.0",
    "node-json-db": "^0.7.3",
    "os": "^0.1.1",
    "ref": "^1.3.4",
    "ref-struct": "^1.1.0",
    "rickshaw": "^1.6.0",
    "riot": "^2.3.15"
  },
  "build": {
    "appId": "org.fermiumlabs.datalogger",
    "asarUnpack": [
      "./app/devices/**/**"
    ],
    "mac": {
      "category": "public.app-category.utilities"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm",
        "tar.gz"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "squirrel",
        "zip"
      ]
    }
  }
}
