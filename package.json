{
  "name": "Datalogger",
  "version": "1.0.0",
  "description": "A datalogger application",
  "main": "app/main.js",
  "scripts": {
    "start": " electron .",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist:linux": "electron-builder --linux",
    "dockerdist:linux": "docker run --rm -ti -v ${PWD}:/project -v ${PWD##*/}-node-modules:/project/node_modules -v ~/.electron:/root/.electron electronuserland/electron-builder:wine /bin/bash -c \"yarn && yarn dist:linux\"",
    "dist:mac": "electron-builder --macos",
    "dockerdist:mac": "docker run --rm -ti -v ${PWD}:/project -v ${PWD##*/}-node-modules:/project/node_modules -v ~/.electron:/root/.electron electronuserland/electron-builder:wine /bin/bash -c \"yarn && yarn dist:mac\"",
    "dist:win": "electron-builder --win",
    "dockerdist:win": "docker run --rm -ti -v ${PWD}:/project -v ${PWD##*/}-node-modules:/project/node_modules -v ~/.electron:/root/.electron electronuserland/electron-builder:wine /bin/bash -c \"yarn && yarn dist:win\"",
    "dist:all": "electron-builder --linux --win",
    "dockerdist:all": "docker run --rm -ti -v ${PWD}:/project -v ${PWD##*/}-node-modules:/project/node_modules -v ~/.electron:/root/.electron electronuserland/electron-builder:wine /bin/bash -c \"yarn && yarn dist:all\""
  },
  "postinstall": "install-app-deps",
  "repository": "https://github.com/fermiumlabs/datalogger",
  "keywords": [],
  "author": {
    "name": "Simone Tosato",
    "email": "s@fermiumlabs.com",
    "url": "https://fermiumlabs.com"
  },
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.23.0",
    "devtron": "^1.4.0",
    "electron": "^1.6.6",
    "electron-builder": "^19.13.0",
    "electron-builder-squirrel-windows": "^16.6.0",
    "gulp-concat": "^2.6.0"
  },
  "dependencies": {
    "bootstrap-slider": "^9.7.2",
    "chart.js": "^2.5.0",
    "codemirror": "^5.25.0",
    "d3": "^4.7.3",
    "data-chan": "^0.3.4",
    "dateformat": "^2.0.0",
    "easytimer": "^1.1.1",
    "echarts": "^3.4.0",
    "electron-pdf-window": "^1.0.10",
    "electron-titlebar": "0.0.3",
    "fs-path": "0.0.23",
    "js-yaml": "^3.8.2",
    "json2csv": "^3.7.3",
    "json2xls": "^0.1.2",
    "jsonfile": "^2.4.0",
    "lodash": "^4.17.4",
    "lowdb": "^0.16.0",
    "mathjax-electron": "^1.2.0",
    "mathjs": "^3.10.0",
    "mousetrap": "^1.6.1",
    "node-json-db": "^0.7.3",
    "os": "^0.1.1",
    "raven": "^2.0.1",
    "ref": "^1.3.4",
    "ref-struct": "^1.1.0",
    "rickshaw": "^1.6.0",
    "riot": "^2.3.15"
  },
  "build": {
    "appId": "org.fermiumlabs.datalogger",
    "asarUnpack": [
      "./app/devices/**/**",
      "./node_modules/data-chan/libDataChan*"
    ],
    "mac": {
      "category": "public.app-category.utilities"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "squirrel",
        "zip"
      ]
    }
  }
}
